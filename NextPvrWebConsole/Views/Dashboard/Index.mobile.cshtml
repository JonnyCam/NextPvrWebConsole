@{
    Layout = null;
}
<!doctype html>
<html>
<head>
    <meta charset="utf-8" />

    <!-- Always force latest IE rendering engine (even in intranet) & Chrome Frame 
         Remove this if you use the .htaccess -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <title>NextPVR Web Console</title>

    <meta name="description" content="NextPVR Web Console">
    <meta name="author" content="reven.co.nz">
  
    <!--  Mobile viewport optimized -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    
  
    @Styles.Render("~/Content/mobile/css")        
       
    @Scripts.Render("~/bundles/mobile/js")     

    @Scripts.Render("~/languages/en.js")       

    <script type="text/javascript">

        $.mobile.defaultPageTransition = 'none';

    </script>
</head>

<body>
    <div id="appLoading" class="appLoading"></div>

    <div data-role="page" id="masterpage" data-theme="a">
    
	    <div data-role="footer" data-position="fixed" data-theme="c" data-tap-toggle="false" id="footer">
            <div data-role="navbar">
                <ul>
                    <li><a id="pagetab_home" data-icon="home" href="#home">Dashboard</a></li>
                    <li><a id="pagetab_guide" data-icon="guide" href="#guide" data-load-script="initialLoadEpgPage()">Guide</a></li>
                    <li><a id="pagetab_recordings" data-icon="recordings" href="#recordings">Recordings</a></li>
                    <li><a id="pagetab_configuration" data-icon="config" href="#configuration">Config</a></li>
                </ul>
            </div>
        </div>
    </div>


</body>
</html>